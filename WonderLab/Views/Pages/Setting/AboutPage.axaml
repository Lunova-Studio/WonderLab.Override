<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:wc="using:WonderLab.Controls"
			 xmlns:wa="using:WonderLab.Media.Attachments"
			 xmlns:mi="using:Material.Icons.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="WonderLab.AboutPage">
	<ItemsControl Margin="0 0 16 24">
		<ItemsControl.ItemsPanel>
			<ItemsPanelTemplate>
				<StackPanel Spacing="8"/>
			</ItemsPanelTemplate>
		</ItemsControl.ItemsPanel>

		<wc:SettingCards Header="关于">
			<wc:SettingCard Header="版本"
							Icon="{mi:MaterialIconExt InformationOutline}">
				<HyperlinkButton Content="2.0.0"
								 NavigateUri="C:\Users\wxysd\Downloads\betterncm_installer.exe"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="赞助我们"
							Icon="{mi:MaterialIconExt GiftOutline}">
				<HyperlinkButton Content="爱发电"
								 NavigateUri="https://afdian.com/a/WonderLab"/>
			</wc:SettingCard>
		</wc:SettingCards>

		<wc:SettingCards Margin="0 12 0 0"
						 Header="鸣谢">
			<wc:SettingCard Header="技术帮助"
							Icon="{mi:MaterialIconExt AccountCowboyHatOutline}">
				<StackPanel Spacing="8"
							Orientation="Horizontal">
					<HyperlinkButton Content="Natsurainko"
									 NavigateUri="https://github.com/natsurainko"/>

					<HyperlinkButton Content="llr下一你了亲并"
									 NavigateUri="https://github.com/laolarou726"/>
				</StackPanel>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="部分图标引用自"
							Icon="{mi:MaterialIconExt AccountCowboyHatOutline}">
				<HyperlinkButton Content="pdimu"
								 NavigateUri="https://github.com/pdimu"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="主界面及部分交互逻辑参考自"
							Icon="{mi:MaterialIconExt AccountCowboyHatOutline}">
				<HyperlinkButton Content="LauncherX"
								 NavigateUri="https://www.corona.studio/lx"/>
			</wc:SettingCard>
		</wc:SettingCards>


		<wc:SettingCards Margin="0 12 0 0"
						 Header="使用的开源项目">
			<wc:SettingCard Header="Avalonia"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/AvaloniaUI/Avalonia"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="AsyncImageLoader.Avalonia"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/AvaloniaUtils/AsyncImageLoader.Avalonia"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="Avalonia.Xaml.Behaviors"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/AvaloniaUI/Avalonia.Xaml.Behaviors"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="DialogHost.Avalonia"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/AvaloniaUtils/DialogHost.Avalonia"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="CommunityToolkit.Mvvm"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/CommunityToolkit/dotnet"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="MinecraftLaunch"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/Lunova-Studio/MinecraftLaunch"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="Monet"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/Lunova-Studio/Monet"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="Serilog"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/serilog/serilog"/>
			</wc:SettingCard>

			<Separator Margin="16 0" Padding="0" Height="1" Opacity="0.2"/>

			<wc:SettingCard Header="SkiaSharp"
							Icon="{mi:MaterialIconExt PackageVariantClosed}">
				<HyperlinkButton Content="Github"
								 NavigateUri="https://github.com/mono/SkiaSharp"/>
			</wc:SettingCard>
		</wc:SettingCards>
	</ItemsControl>
</UserControl>